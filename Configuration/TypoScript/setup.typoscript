            stdWrap.HTMLparser.tags.td.fixAttrib.class.list = plain,text-left,text-right,text-justify,text-center
            stdWrap.HTMLparser.tags.th.fixAttrib.class.list = plain,text-left,text-right,text-justify,text-center
        }
    }
}

