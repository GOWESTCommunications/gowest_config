    dataProcessing {
        10 = FriendsOfTYPO3\Headless\DataProcessing\FilesProcessor
        10 {
            references.fieldName = media
            as = media
        }
    }
    overwriteMenuLevelConfig {
        stdWrap.cObject {
            100 = TEXT
            100.field = uid
            100.wrap = ,"uid":|
        }
 }
}

